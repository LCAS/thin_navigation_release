<?xml version="1.0" encoding="UTF-8" ?>
<launch>

  <arg name="map" default="disB1" />

  <arg name="stage_pkg" default="stage_ros" />
  
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>

  <!-- Stage simulator -->


<node name="stageros" pkg="$(arg stage_pkg)" type="stageros" args="$(find thin_navigation)/test/$(arg map).world" output="screen" required="true" /> 

</launch>
